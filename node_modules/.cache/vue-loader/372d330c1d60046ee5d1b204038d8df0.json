 },\n    title: {\n      type: String,\n      default: \"Modal\"\n    },\n    showFooter: {\n      type: Boolean,\n      default: true\n    },\n  },\n  methods: {\n    close() {\n      this.$emit(\"close\"); // Emite um evento para informar que o modal foi fechado\n    },\n  },\n};\n</script>\n\n<style scoped>\n/* Overlay escuro */\n.modal-overlay {\n  position: fixed;\n  top: 0;\n  left: 0;\n  height: 100%;\n  width: 100%;\n  background: rgba(0, 0, 0, 0.5);\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  z-index: 1000;\n}\n\n/* Container principal do modal */\n.modal {\n  background: white;\n  border-radius: 8px;\n  width: 90%;\n  max-width: 500px;\n  padding: 20px;\n  box-shadow: 0px 4px 10px rgba(0, 0, 0, 0.25);\n  text-align: center;\n  position: relative;\n  animation: fadeIn 0.3s ease;\n}\n\n/* Título */\n.modal-title {\n  font-size: 1.5rem;\n  font-weight: bold;\n  margin-bottom: 20px;\n  color: #333;\n}\n\n/* Botão de Fechar */\n.modal-close {\n  position: absolute;\n  top: 10px;\n  right: 10px;\n  font-size: 1.5rem;\n  cursor: pointer;\n  border: none;\n  background: none;\n  color: #666;\n}\n\n.modal-close:hover {\n  color: #000;\n}\n\n/* Estilo da área de conteúdo */\n.modal-content {\n  margin-bottom: 20px;\n}\n\n/* Rodapé (botões) */\n.modal-footer {\n  display: flex;\n  justify-content: space-between;\n  gap: 10px;\n}\n\n.btn-secondary {\n  background-color: #e0e0e0;\n  color: #333;\n  font-size: 1rem;\n  padding: 10px 20px;\n  border: none;\n  border-radius: 5px;\n  cursor: pointer;\n}\n\n.btn-secondary:hover {\n  background-color: #ccc;\n}\n\n.btn-primary {\n  background-color: #003366;\n  color: white;\n  font-size: 1rem;\n  padding: 10px 20px;\n  border: none;\n  border-radius: 5px;\n  cursor: pointer;\n}\n\n.btn-primary:hover {\n  background-color: #0056b3;\n}\n\n/* Animação de FadeIn */\n@keyframes fadeIn {\n  from {\n    transform: scale(0.95);\n    opacity: 0;\n  }\n  to {\n    transform: scale(1);\n    opacity: 1;\n  }\n}\n</style>"]}]}